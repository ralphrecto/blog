---
layout: base.liquid
title: blog
---
<table class="table table-borderless table-sm">
  <tbody>
  {%- for post in collections.posts -%}
    {% if post.data.title != "blog" %}
    <tr>
      <td class="pt-0 pb-2">
        <small style="padding-right: 20px">
          {{ post.date | date: "%B %e, %Y" }}
        </small>
      </td>
      <td class="pt-0 pb-2">
        <h5>
          <a href="{{ post.url }}">{{ post.data.title }}</a>
        </h5>
      </td>
    </tr>
    {% endif %}
  {%- endfor -%}
  </tbody>
</table>
