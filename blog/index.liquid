---
layout: base.liquid
title: blog
---
<table class="table table-borderless table-sm">
  <tbody>
  {%- for post in collections.posts reversed -%}
    {% if post.data.title != "blog" %}
    <tr>
      <td class="pt-0 pb-2" style="padding-left: 0">
        <h5 style="margin-bottom: 0"><a href="{{ post.url }}">{{ post.data.title }}</a></h5>
        {{ post.date | date: "%B %e, %Y" }}
      </td>
    </tr>
    {% endif %}
  {%- endfor -%}
  </tbody>
</table>
