---
layout: base.liquid
title: projects
---
<table class="table table-borderless table-sm">
  <tbody>
  {%- for project in collections.projects -%}
    {% if project.data.title != "projects" %}
    <tr>
      <td class="pt-0 pb-2" style="text-align: right; padding-right: 20px">
        <a href="{{ project.data.link }}">{{ project.data.title }}</a>
      </td>
      <td class="pt-0 pb-2">
        {{ project.templateContent }}
      </td>
    </tr>
    {% endif %}
  {%- endfor -%}
  </tbody>
</table>
